        "karelwintersky/arris.router": "^1.3",
        "karelwintersky/arris.logger": "^1.0",
        "karelwintersky/arris.delight-php-auth": "^0.9.1",
        "karelwintersky/arris.template": "^0.16.0",
        "karelwintersky/arris.cache": "^0.7.1",
        "karelwintersky/arris.entity": "^0.9.3",

        "ajur-media/template.flash": "^0.1.0",
        "mobiledetect/mobiledetectlib": "^3.74",
        "kuria/error": "^4.2",
        "slim/psr7": "^1.6",
        "ajur-media/kcaptcha": "^1.1",
